# 后端环境变量配置 - 开发环境
PORT=3001

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=mockdata
DB_USER=root
DB_PASSWORD=your_db_password

# JWT密钥
JWT_SECRET=mockdata-jwt-secret-key-2024-development

# 会话密钥
SESSION_SECRET=mockdata-session-secret-key-2024

# 应用环境
NODE_ENV=development

# CORS配置
CORS_ORIGIN=http://localhost:3002

# 文件上传配置
UPLOAD_MAX_SIZE=10485760
UPLOAD_PATH=./uploads

# 日志级别
LOG_LEVEL=debug